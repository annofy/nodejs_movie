{
	// 控制缺少分号的警告 asi: true
	"boss": false,
	// 循环或者条件语句必须用花括号包围
	"curly": true,
	// 强制使用===
	"eqeqeq": true,
	// 
	"eqnull": true,
	"expr": true,
	"immed": true,
	// 构造器函数首字母大写
	"newcap": true,
	"noempty": true,
	// 禁止使用 arguments.caller 和 arguments.callee，未来会被弃用， ECMAScript 5 禁止使用 arguments.callee
	"noarg": true,
	// 变量未定义
	"undef": true,
	"regexp": true,
	// 预定义全局的变量 document, navigator, FileReader等
	"browser": true,
	// 定义用于调试的全局变量：console，alert
	"devel": true,
	// 定义全局变量 
	"node": true,
	// 兼容低级浏览器 ie6/7/8
	"es3": true
	// frezze: true 禁止重写原生对象的原型
	// indent: true 代码缩进
	// latedef: 'nofunc' 禁止定义之前使用变量，忽略 function 函数声明
	//  quotmark: true 为 true 时，禁止单引号和双引号混用
	//  strict: true 严格模式
	// maxparams: 4 最多四个参数
	// maxdepth:4 最大嵌套深度为4
	// maxcomplexity: true 复杂度检测
	// maxlen: 600 最大行数600
	// multistr: true 检查多行字符串
	// notypeof: true 检查无效的typeof操作符

}